{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Message",
	"description": "Format for messages on a link",
	"type": "object",
	"additionalProperties": false,
	"required": ["seq", "type", "data"],
	"properties": {
		"seq": { "type": ["integer", "null"] },
		"type": { "type": "string" },
		"data": { "type": "object" }
	}
}